<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.michelle.driverbuddy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Logging">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DriverMenu" />
        <activity android:name=".DriverEditProfile" />
        <activity android:name=".insurance_profile" />
        <activity android:name=".insurance_edit_profile" />

        <activity android:name=".Insurance_registration" />
        <activity android:name=".insurance_track_driver" />
        <activity android:name=".insurance_popwindow"
            android:theme="@style/AppTheme.CustomTheme">
        </activity>

    </application>

</manifest>
